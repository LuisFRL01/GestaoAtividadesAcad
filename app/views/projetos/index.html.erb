
<div class="container-fluid textoCentralizado projetos rotulo">
  <h2><strong>Projetos</strong></h2>
  <div class="row-sm">
    <table class="table table-hover table-dark">
      <thead>
      <tr>
        <th scope="col">Nome</th>
        <th scope="col">√Årea</th>
        <th scope="col">Natureza</th>
        <th scope="col">Status</th>
        <th scope="col"></th>
        <th scope="col"><%= link_to "",new_projeto_path, :class =>"oi oi-plus"%></th>
      </tr>
      </thead>
      <tbody>
      <% @projetos.each do |projeto| %>
        <tr>
          <td><%= link_to projeto.titulo, projeto, id:"s-#{projeto.titulo}" %></td>
          <td><%= projeto.area %></td>
          <td><%= projeto.tipoProjeto %></td>
          <td><strong></strong></td>
          <td><strong><%= link_to 'Editar', edit_projeto_path(projeto)%></strong></td>
          <td><strong><%= link_to 'Cancelar', projeto,id:"d-#{projeto.titulo}", method: :delete %></strong></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>


<p id="notice"><%= notice %></p>



