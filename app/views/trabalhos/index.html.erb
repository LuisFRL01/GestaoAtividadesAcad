<p id="notice"><%= notice %></p>

<h1>Integrantes</h1>

<table>
  <thead>
    <tr>
      <th>Função</th>
      <th>E-mail</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <%@trabalhos.each do |trabalho| %>
      <tr>
        <td><%= trabalho.funcao %></td>
        <td><%= trabalho.usuario.email %></td>
        <td><strong><%= link_to 'Remover integrante', [trabalho.projeto,trabalho],id:"d-#{trabalho.projeto_id}" ,method: :delete%></strong>
        <td><strong></strong></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'voltar', projeto_path(@projeto) %>